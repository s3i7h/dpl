{
  "name": "deepl-cli",
  "version": "0.0.17",
  "description": "Use deepl with cli.",
  "main": "lib/index.js",
  "scripts": {
    "start": "node bin/cli.js",
    "config": "node bin/cli.js setting init",
    "build": "esbuild lib/index.js --bundle --platform=node --target=node18 --outfile=dist/bundle.js && pkg dist/bundle.js -t node18-macos-arm64 -o dist/dpl",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/shu-pf/dpl.git"
  },
  "author": "Shugo Furuse <shugoena@gmail.com> (https://github.com/shu-pf)",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.3",
    "inquirer": "^8.0.0"
  },
  "bin": {
    "dpl": "bin/cli.js"
  },
  "devDependencies": {
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.10",
    "jest": "^29.7.0",
    "pkg": "^5.8.1"
  }
}
